{
  "name": "@scalvert/readme-api-generator",
  "version": "0.2.0",
  "description": "Generates API documentation in your README",
  "repository": {
    "type": "git",
    "url": "https://github.com/scalvert/readme-api-generator.git"
  },
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "readme-api-generator": "./bin/readme-api-generator.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "yarn lint && vitest run",
    "docs:generate": "node ./bin/readme-api-generator.js index.js -m CUSTOM"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^6.0.1",
    "meow": "^9.0.0",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@scalvert/bin-tester": "^0.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.0",
    "release-it": "^14.2.1",
    "release-it-lerna-changelog": "^3.1.0",
    "vite": "^2.8.6",
    "vitest": "^0.7.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=14"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "14.17.6",
    "yarn": "1.22.11"
  }
}
